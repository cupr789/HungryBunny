<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="payCa">
	<insert id="insertPayCa" parameterType="map">
		insert into pay_category(payType) 
		values('휴대폰');
	</insert> 
	<select id="selectPayCa" resultType="payCa">
		select * from pay_category order by payCaNo;
	</select>
</mapper>  